{
  "title": "NJUCSer成长手册",
  "language": "zh-hans",
  "author": "yxz",
  "plugins": [
    "-search",
    "-lunr",
    "-sharing",
    "-livereload",
    "github",
    "chart",
    "todo",
    "graph",
    "puml",
    "katex",
    "code",
    "ace",
    "flexible-alerts",
    "chapter-fold",
    "anchor-navigation-ex",
    "theme-comscore",
    "hints",
    "pageview-count"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": false,
      "showGoTop": true
    },
    "github": {
      "url": "https://github.com/NJU-CS/njucs"
    },
    "edit-link": {
      "base": "https://github.com/NJU-CS/njucs",
      "label": "Edit This Page"
    }
  }
}
